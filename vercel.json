{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/routes/index.js"
      }
    ],
    "env": {
        "DB_DIALECT": "@db_dialect",
        "SECRET_KEY": "@secret_key"
      }
}